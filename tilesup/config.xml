<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lagache.tilesup" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TilesUp</name>
    <description>
        A react-redux version of Simon says.
    </description>
    <author email="christophe.gachiniard@gmail.com" href="https://github.com/lagache/react-redux-fiddling">
        La gache
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/tilesup.png" platform="android" width="57" height="57" density="mdpi" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>